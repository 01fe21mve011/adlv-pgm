module string_datatype;
  string s1="Hello word";
  string s2={"Hi","",s1};
  bit[31:0]b=128;
  string s3=b;
  initial begin
    $display("string 1 s1=%0s",s1);
    $display("string 2 s2=%0s",s2);
    $display("string 3 s1=%0d bit b=%d",s3,b);
  end
endmodule
